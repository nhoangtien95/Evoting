@{
    var candidates = ViewBag.Candidates as List<Evoting.Models.Candidate>;
    var rndcandidates = ViewBag.RndCandidate as Evoting.Models.Candidate;
}
<!DOCTYPE html>
<html>

<!-- Mirrored from coffeecreamthemes.com/themes/jobseek/site/candidates.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 30 Jul 2015 04:55:17 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Jobseek - Job Board Responsive HTML Template">
    <meta name="author" content="Coffeecream Themes, info@coffeecream.eu">
    <title>Candidates List</title>
    <link rel="shortcut icon" href="/Content/images/favicon.png">

    <!-- Main Stylesheet -->
    <link href="/Content/CSS/style.css" rel="stylesheet">

    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->

</head>
<body style="margin-top: -200px">

    <!-- ============ PAGE LOADER START ============ -->

    <div id="loader">
        <i class="fa fa-cog fa-4x fa-spin"></i>
    </div>

    <!-- ============ PAGE LOADER END ============ -->
    <!-- ============ TITLE START ============ -->

    <section id="title">
        <div class="container jumbotron">
            <div class="row">
                <div class="col-sm-12 text-center">
                    <h1>Candidates</h1>
                </div>
            </div>
        </div>
    </section>

    <!-- ============ TITLE END ============ -->
    <!-- ============ JOBS START ============ -->

    <section id="jobs">
        <div class="container">
            <div class="row">
                <div class="col-sm-8">
                    <h2>Candidates</h2>

                    <div class="jobs">
                        @foreach (var item in candidates)
                        {
                            <!-- Job offer 1 -->
                            <a href="@Url.Action("CandidateDetail", "Candidates", new { _id = item.ID})">
                                <img src="/Images/@item.Photo" alt="" class="img-circle" style="max-width: 100px; max-height: 100px; width: 100px; height: 100px" />
                                <div class="title">
                                    <h5>@item.Name</h5>
                                    <p>@item.Degree</p>
                                </div>
                                <div class="data">
                                    <span class="city"><i class="fa fa-map-marker"></i>@item.Address</span>
                                    <span class="experience"><i class="fa fa-trophy"></i>TDT University</span>
                                </div>
                            </a>
                        }                     

                    </div>


                </div>
                <div class="col-sm-4" id="sidebar">

                    <!-- Featured Jobs Start -->
                    <div class="sidebar-widget">
                        <h2>Featured Candidate</h2>
                        <a href="@Url.Action("CandidateDetail", new { _id = @rndcandidates.ID})">
                            @*<img src="/Content/images/featured-job.jpg" alt="Featured Job" class="img-responsive" />*@
                            <div class="featured-job">
                                <img src="/Images/@rndcandidates.Photo" alt="" class="img-circle pull-left" style="max-width: 200px; max-height: 200px; width: 200px; height: 200px" />
                                <div class="title">
                                    <h5>@rndcandidates.Name</h5>
                                    <p>@rndcandidates.Degree</p>
                                </div>
                                <div class="data">
                                    <span class="city"><i class="fa fa-map-marker"></i>@rndcandidates.Address</span>
                                    <span class="experience"><i class="fa fa-trophy"></i>TDT University</span>
                                </div>
                                <div class="description">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Pellentesque tristique euismod lorem, a consequat orci consequat a. Donec ullamcorper tincidunt nunc, ut aliquam est pellentesque porta. In neque erat, malesuada sit amet orci ac, laoreet laoreet mauris.</div>
                            </div>
                        </a>
                    </div>
                    <!-- Featured Jobs End -->

                </div>
            </div>
        </div>
    </section>

    <!-- ============ JOBS END ============ -->
    <!-- ============ FOOTER START ============ -->

    <footer>
        <div id="credits">
            <div class="container text-center">
                <div class="row">
                    <div class="col-sm-12">
                        &copy; 2017 51303176 - EVoting System<br>
                        Designed &amp; Developed by <a href="#" target="_blank">51303176</a>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- ============ FOOTER END ============ -->
    <!-- Modernizr Plugin -->
    <script src="/Content/js/modernizr.custom.79639.js"></script>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="/Content/js/jquery-1.11.2.min.js"></script>

    <!-- Bootstrap Plugins -->
    <script src="/Content/js/bootstrap.min.js"></script>

    <!-- Retina Plugin -->
    <script src="/Content/js/retina.min.js"></script>

    <!-- ScrollReveal Plugin -->
    <script src="/Content/js/scrollReveal.min.js"></script>

    <!-- Flex Menu Plugin -->
    <script src="/Content/js/jquery.flexmenu.js"></script>

    <!-- Slider Plugin -->
    <script src="/Content/js/jquery.ba-cond.min.js"></script>
    <script src="/Content/js/jquery.slitslider.js"></script>

    <!-- Carousel Plugin -->
    <script src="/Content/js/owl.carousel.min.js"></script>

    <!-- Parallax Plugin -->
    <script src="/Content/js/parallax.js"></script>

    <!-- Counterup Plugin -->
    <script src="/Content/js/jquery.counterup.min.js"></script>
    <script src="/Content/js/waypoints.min.js"></script>

    <!-- No UI Slider Plugin -->
    <script src="/Content/js/jquery.nouislider.all.min.js"></script>

    <!-- Bootstrap Wysiwyg Plugin -->
    <script src="/Content/js/bootstrap-wysiwyg.js"></script>
    <script src="/Content/js/jquery.hotkeys.js"></script>

    <!-- Flickr Plugin -->
    <script src="/Content/js/jflickrfeed.min.js"></script>

    <!-- Fancybox Plugin -->
    <script src="/Content/js/fancybox.pack.js"></script>

    <!-- Magic Form Processing -->
    <script src="/Content/js/magic.js"></script>

    <!-- jQuery Settings -->
    <script src="/Content/js/settings.js"></script>


</body>

