<body>

    <!-- ============ PAGE LOADER START ============ -->

    <div id="loader">
        <i class="fa fa-cog fa-4x fa-spin"></i>
    </div>

    <!-- ============ JOBS START ============ -->

    <section id="jobs" style="max-height: 0px;">
        <div class="container">
            <div class="row text-center">
                <div class="col-sm-12">
                    <h1>Welcome to Evote</h1>
                    <h4>Find a Right Candidate</h4>
                    <div class="jumbotron">
                        <h3>Have an account?</h3>
                        <p>
                            If you have an account you can sign in to vote now .<br>
                            If you don't have you can create one by clicking sign up and entering your ID.
                        </p>
                        <p><a href="@Url.Action("SignIn", new { })"  class="btn btn-primary">Vote Now</a>
                           <a href="@Url.Action("SignUp", new { })" class="btn btn-primary">Sign Up</a></p> 
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Modernizr Plugin -->
    <script src="/Content/js/modernizr.custom.79639.js"></script>

